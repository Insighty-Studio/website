.landscape-screen
  display none

  +iphone-landscape()
    display flex
    align-items center
    justify-content center
    background-color #FFFFFF
    height 100vh
    width 100vw

    .warning
      display flex
      flex-direction column
      align-items center

      .warning-message
        margin-top 24px
        font-family Roboto Mono
        font-size 21px
