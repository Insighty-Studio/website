.partners
  display grid
  grid-template-columns repeat(3, 1fr)
  align-items center
  justify-items center
  grid-gap 80px
  padding 120px 0
  background-color rgba(0,0,0, .1)

  +iphone()
    padding 64px 0
    grid-gap 64px
    grid-template-columns repeat(1, 6fr)
    flex-direction column
    background-color white

  +iphone5()
    padding 58px 0
    grid-gap 58px
    grid-template-columns repeat(1, 6fr)
    flex-direction column
    background-color white

  svg:last-child
    margin 0
